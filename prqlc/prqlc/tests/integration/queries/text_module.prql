# mssql:test
from db.albums
select {
    title,
    title_and_spaces = f"  {title}  ",
    low = title | std.text.lower,
    up = title | std.text.upper,
    ltrimmed = title | std.text.ltrim,
    rtrimmed = title | std.text.rtrim,
    trimmed = title | std.text.trim,
    len = title | std.text.length,
    subs = title | std.text.extract 2 5,
    replace = title | std.text.replace "al" "PIKA",
}
sort {title}
filter (title | std.text.starts_with "Black") || (title | std.text.contains "Sabbath") || (title | std.text.ends_with "os")

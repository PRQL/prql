# mssql:test
# sqlite:skip (see https://github.com/rusqlite/rusqlite/issues/1211)
from db.invoices
take 5
select {
    total_original = total | std.math.round 2,
    total_x = std.math.pi - total | std.math.round 2 | std.math.abs,
    total_floor = std.math.floor total,
    total_ceil = std.math.ceil total,
    total_log10 = std.math.log10 total | std.math.round 3,
    total_log2 = std.math.log 2 total | std.math.round 3,
    total_sqrt = std.math.sqrt total | std.math.round 3,
    total_ln = std.math.ln total | std.math.exp | std.math.round 2,
    total_cos = std.math.cos total | std.math.acos | std.math.round 2,
    total_sin = std.math.sin total | std.math.asin | std.math.round 2,
    total_tan = std.math.tan total | std.math.atan | std.math.round 2,
    total_deg = total | std.math.degrees | std.math.radians | std.math.round 2,
    total_square = total | std.math.pow 2| std.math.round 2,
}

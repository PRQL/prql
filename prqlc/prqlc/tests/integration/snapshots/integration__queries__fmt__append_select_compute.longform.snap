input_file: prqlc/prqlc/tests/integration/queries/append_select_compute.longform.prql
---
from invoices
select {customer_id, invoice_id, total}
take 5
append (
  from invoice_items
  select {invoice_line_id, invoice_id, unit_price}
  take 5
)
select {
  a = customer_id * 2,
  b = math.round 1 invoice_id * total,
}

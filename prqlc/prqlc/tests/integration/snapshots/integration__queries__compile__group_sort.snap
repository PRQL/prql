---
source: prqlc/prqlc/tests/integration/queries.rs
expression: "# mssql:test\nfrom tracks\nderive d = (album_id + 1 | as REAL)\ngroup d (\n    aggregate {\n        n1 = (track_id | sum | as REAL),\n    }\n)\nsort d\ntake 10\nselect { d1 = d, n1 }\n"
input_file: prqlc/prqlc/tests/integration/queries/group_sort.prql
---
WITH table_0 AS (
  SELECT
    CAST(COALESCE(SUM(track_id), 0) AS REAL) AS n1,
    CAST(album_id + 1 AS REAL) AS _expr_0
  FROM
    tracks
  GROUP BY
    CAST(album_id + 1 AS REAL)
),
table_1 AS (
  SELECT
    _expr_0 AS d1,
    n1,
    _expr_0
  FROM
    table_0
  ORDER BY
    _expr_0
  LIMIT
    10
)
SELECT
  d1,
  n1
FROM
  table_1
ORDER BY
  _expr_0

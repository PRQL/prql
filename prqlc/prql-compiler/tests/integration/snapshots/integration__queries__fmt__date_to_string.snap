expression: "# generic:skip\n# glaredb:skip\n# clickhouse:skip\n# mssql:skip\n# postgres:skip\n# mysql:test\n# duckdb:skip\n# sqlite:skip\nfrom invoices\ntake 20\nselect {\n    d1 = invoice_date | date.to_string \"%Y/%m/%d\",\n    d2 = invoice_date | date.to_string \"%F\",\n    d3 = invoice_date | date.to_string \"%v\",\n    d4 = invoice_date | date.to_string \"%x\",\n    d5 = invoice_date | date.to_string \"%+\",\n    d6 = invoice_date | date.to_string \"%c\",\n    d7 = invoice_date | date.to_string \"%r\",\n    d8 = invoice_date | date.to_string \"%A %B %d %Y\",\n    d9 = invoice_date | date.to_string \"%-d/%-m/%y %-H:%M:%S\",\n    d10 = invoice_date | date.to_string \"%Mmin %Ssec\",\n    d11 = invoice_date | date.to_string \"%M'%S\\\"\",\n}\n"
input_file: prqlc/prql-compiler/tests/integration/queries/date_to_string.prql
---
from invoices
take 20
select {
  d1 = (invoice_date | date.to_string "%Y/%m/%d"),
  d2 = (invoice_date | date.to_string "%F"),
  d3 = (invoice_date | date.to_string "%v"),
  d4 = (invoice_date | date.to_string "%x"),
  d5 = (invoice_date | date.to_string "%+"),
  d6 = (invoice_date | date.to_string "%c"),
  d7 = (invoice_date | date.to_string "%r"),
  d8 = (
    invoice_date
    date.to_string "%A %B %d %Y"
  ),
  d9 = (
    invoice_date
    date.to_string "%-d/%-m/%y %-H:%M:%S"
  ),
  d10 = (
    invoice_date
    date.to_string "%Mmin %Ssec"
  ),
  d11 = (invoice_date | date.to_string ""%M'%S"""),
}


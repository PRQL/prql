input_file: prqlc/prql-compiler/tests/integration/queries/date_to_string.prql
---
from invoices
take 20
select {
  d1 = (invoice_date | date.to_string "%Y/%m/%d"),
  d2 = (invoice_date | date.to_string "%F"),
  d3 = (invoice_date | date.to_string "%D"),
  d4 = (invoice_date | date.to_string "%H:%M:%S.%f"),
  d5 = (invoice_date | date.to_string "%r"),
  d6 = (invoice_date | date.to_string "%A %B %-d %Y"),
  d7 = (invoice_date | date.to_string "%a, %-d %b %Y at %I:%M:%S %p"),
  d8 = (invoice_date | date.to_string "%+"),
  d9 = (invoice_date | date.to_string "%-d/%-m/%y"),
  d10 = (invoice_date | date.to_string "%-Hh %-Mmin"),
  d11 = (invoice_date | date.to_string ""%-M'%S"""),
  d12 = (invoice_date | date.to_string """%-M''""'""""),
}


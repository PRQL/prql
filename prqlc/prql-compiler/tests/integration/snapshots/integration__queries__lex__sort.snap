 from e = employees 
 filter first_name != "Mitchell" 
 sort { first_name , last_name } 
 
 
 join manager = employees side : left ( e . reports_to == manager . employee_id ) 
 
 select { e . first_name , e . last_name , manager . first_name } 
 

let long_query = (
    from employees
    filter gross_cost > 0
    group {title} (
        aggregate {
            ct = count s"*",
        }
    )
    sort ct
    filter ct > 200
    take 20
    sort ct
    filter ct > 200
    take 20
    sort ct
    filter ct > 200
    take 20
    sort ct
    filter ct > 200
    take 20
)
WITH newest_employees AS (
  SELECT
    TOP (50) *
  FROM
    employees
  ORDER BY
    tenure
),
average_salaries AS (
  SELECT
    country,
    AVG(salary) AS average_country_salary
  FROM
    salaries
  GROUP BY
    country
)
SELECT
  name,
  salary,
  average_country_salary
FROM
  newest_employees
  JOIN average_salaries USING(country)

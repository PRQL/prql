prql dialect:generic

from `employees`
filter `country` == "USA"
derive [
  gross_salary = `salary` + `payroll_tax`,
  gross_cost = `gross_salary` + `benefits_cost`,
]
filter `gross_cost` > 0
group [
  `title`,
  `country`,
] (
  aggregate [
  average `salary`,
  average `gross_salary`,
  sum `salary`,
  sum `gross_salary`,
  average `gross_cost`,
  sum_gross_cost = (sum `gross_cost`),
  ct = `count`,
]
)
sort `sum_gross_cost`
filter `ct` > 200
take 20



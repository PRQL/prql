WITH table_0 AS (
  SELECT
    employees.*,
    ROW NUMBER() OVER (
      PARTITION BY department
      ORDER BY
        age
    ) AS _rn
  FROM
    employees
)
SELECT
  table_0.*
FROM
  table_0
WHERE
  _rn <= 1

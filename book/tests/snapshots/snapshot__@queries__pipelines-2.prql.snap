---
source: book/tests/snapshot.rs
expression: "from e = employees\nderive gross_salary = (salary + payroll_tax)\nsort gross_salary\ntake 10\njoin d = department [==dept_no]\nselect [e.name, gross_salary, d.name]\n"
input_file: book/tests/prql/queries/pipelines-2.prql
---
WITH table_1 AS (
  SELECT
    name,
    salary + payroll_tax AS gross_salary,
    dept_no
  FROM
    employees AS e
  ORDER BY
    gross_salary
  LIMIT
    10
)
SELECT
  table_1.name,
  table_1.gross_salary,
  d.name
FROM
  table_1
  JOIN department AS d ON table_1.dept_no = d.dept_no

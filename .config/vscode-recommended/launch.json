  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "prqlc _a.prql",
      "program": "${workspaceFolder}/target/debug/prqlc",
      "args": ["debug", "semantics", "_a.prql"],
      "env": {
        "RUST_LOG": "debug",
        "RUST_BACKTRACE": "1"
      },
      "preLaunchTask": "prqlc-build",
      "cwd": "${workspaceFolder}"
    }
  ]
}

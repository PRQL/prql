---
source: web/book/tests/snapshot.rs
expression: "from web\nselect url = f\"http{tls}://www.{domain}.{tld}/{page}\"\n"
---
SELECT
  CONCAT(
    CONCAT(
      CONCAT(
        CONCAT(
          CONCAT(CONCAT(CONCAT('http', tls), '://www.'), domain),
          '.'
        ),
        tld
      ),
      '/'
    ),
    page
  ) AS url
FROM
  web


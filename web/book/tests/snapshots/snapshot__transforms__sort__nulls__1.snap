---
source: web/book/tests/snapshot.rs
expression: "from artists\nsort {artist_id == null, artist_id} # nulls last\n"
---
WITH table_1 AS (
  SELECT
    *,
    artist_id IS NULL AS _expr_0
  FROM
    artists
)
SELECT
  *
FROM
  table_1 AS table_0
ORDER BY
  _expr_0 NULLS LAST,
  artist_id NULLS LAST


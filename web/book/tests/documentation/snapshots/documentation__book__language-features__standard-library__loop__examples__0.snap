---
source: web/book/tests/documentation/book.rs
expression: "from [{n = 1}]\nloop (\n    filter n<4\n    select n = n+1\n)\n\n# returns [1, 2, 3, 4]\n"
---
WITH RECURSIVE table_5 AS (
  SELECT
    1 AS n
),
table_3 AS (
  SELECT
    n
  FROM
    table_5 AS table_1
  UNION
  ALL
  SELECT
    n + 1
  FROM
    table_3 AS table_2
  WHERE
    n < 4
)
SELECT
  n
FROM
  table_3 AS table_4


---
source: web/book/tests/documentation/book.rs
expression: "from.employees\nderive {first_name, dept_id}\njoin (from.departments | select {d = this}) (==dept_id)\nselect {first_name, d.title}\n"
---
WITH table_0 AS (
  SELECT
    *
  FROM
    departments
)
SELECT
  employees.first_name,
  table_0.title
FROM
  employees
  JOIN table_0 ON employees.dept_id = table_0.dept_id

